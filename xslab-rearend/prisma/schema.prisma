generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("database_url")
}

model xs_canvas {
  id         Int      @id @default(autoincrement())
  name       String
  data       String @db.Text
  options    String @db.Text
  createTime DateTime @default(now())
  delete     Int      @default(1)
  state      Int      @default(1)
}

model xs_database {
  id         Int      @id @default(autoincrement())
  name       String
  type       Int
  ipaddress  String
  port       Int
  username   String
  password   String
  schemas    String
  createTime DateTime @default(now())
  delete     Int      @default(1)
  state      Int      @default(1)
}
