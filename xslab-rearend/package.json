{
  "name": "xscollect",
  "version": "1.0.2",
  "description": "xslab后端，有问题提issues！",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "initDB:dev": "dotenv -e .env.development -- npx prisma migrate dev --name init",
    "dev": "export NODE_ENV=development && nodemon --exec babel-node src/index.js",
    "db-studio": "dotenv -e .env.dev -- npx prisma studio",
    "build": "babel src -d bin && dotenv -e .env.production -- npx prisma db push && dotenv -e .env.production -- npx prisma generate",
    "prod": "npm run build && export NODE_ENV=production && node bin/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:meterXu/xsCollect.git"
  },
  "author": "isaac <xhgainxq@qq.com>",
  "dependencies": {
    "@prisma/client": "4.8.0",
    "art-template": "^4.13.2",
    "commander": "^3.0.1",
    "compressing": "^1.5.1",
    "dotenv": "^16.0.3",
    "image-size": "^1.0.2",
    "koa": "^2.11.0",
    "koa-body": "^4.1.1",
    "koa-router": "^7.4.0",
    "koa-send": "^5.0.0",
    "koa-static": "^5.0.0",
    "koa-swagger-decorator": "^1.8.6",
    "koa2-cors": "^2.0.6",
    "mssql": "^5.1.0",
    "mysql": "^2.17.1",
    "oracledb": "^5.0.0",
    "safe-buffer": "^5.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.7",
    "@babel/node": "^7.20.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.20.7",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "dotenv-cli": "^6.0.0",
    "nodemon": "^2.0.20",
    "prisma": "^4.8.0"
  }
}
